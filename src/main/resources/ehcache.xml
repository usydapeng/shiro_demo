<?xml version="1.0" encoding="UTF-8"?>
<ehcache>

	<diskStore path="C:\Users\usyda_000\Desktop\cache\hello"/>

	<cache name="org.hibernate.cache.StandardQueryCache"
	       maxEntriesLocalHeap="1"
	       eternal="false"
	       timeToIdleSeconds="300"
